{
  "name": "lmo-Transform",
  "version": "1.0.0",
  "main": "./build/electron-app.js",
  "license": "Apache",
  "homepage": ".",
  "author": "ayuanlmo",
  "build": {
    "productName": "lmo-Transform",
    "icon": "public/favicon.ico",
    "asar": true,
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "nsis": {
      "oneClick": false,
      "language": "2052",
      "perMachine": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "icon": "public/icon.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "extends": null,
    "files": [
      "./build/**/*",
      "./build/electron-app.js",
      "./ffmpeg/*"
    ],
    "extraFiles": [
      {
        "from": "ffmpeg",
        "to": "ffmpeg",
        "filter": ["**/*"]
      }
    ]
  },
  "scripts": {
    "start": "tsc && concurrently \"yarn react-start\" \"set NODE_ENV=development && electron build/electron-app.js \"",
    "build": "tsc && set NODE_ENV=production && electron-builder && node build/generate-hash.js",
    "react-start": "set BROWSER=none&&set PORT=3000&&react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "concurrently": "^8.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs": "^0.0.1-security",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-scripts": "5.0.1",
    "typescript": "^5.1.6",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "electron": "^26.0.0",
    "electron-builder": "^24.6.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/electron": "^1.6.10",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.39",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
